{
    "compilerOptions": {
        "allowUnreachableCode": false,
        "declaration": true,
        "esModuleInterop": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "module": "es2015",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "pretty": true,
        "sourceMap": true,
        "strict": true,
        "target": "es2015"
    }
}